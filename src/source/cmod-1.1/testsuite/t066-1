setenv zero1 ""
setenv zero2 ""
setenv zero3 ""
setenv zero4 ""
setenv zero5 ""
setenv zero6 ""
setenv data foo
setenv a "${unset1:-unset produces a}"
setenv b "${unset2-unset produces b}"
setenv c "${zero1:-empty produces c}"
setenv d "${zero2-empty produces d}"
setenv e "${unset3:=unset produces e}"
setenv f "${unset4=unset produces f}"
setenv g "${zero3:=empty produces g}"
setenv h "${zero4=empty produces h}"
setenv i "${unset5:+unset produces i}"
setenv j "${unset6+unset produces j}"
setenv k "${zero5:+empty produces k}"
setenv l "${zero6+empty produces l}"
setenv m "${data:-barm}"
setenv n "${data-barn}"
setenv o "${data:=baro}"
setenv p "${data=burp}"  # To much pizza will make you burp.
setenv q "${data:+barq}"
setenv r "${data+barr}"
